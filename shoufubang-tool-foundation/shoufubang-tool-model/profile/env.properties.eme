# 测试环境(紧急）

# 记录应用日志的跟目录 -- FOR logback
log.home=/data/application/logs/shoufubang
log.root.level=DEBUG
# 数据连接配置 jndi
dataSource.jndi.master=java:comp/env/jdbc/master
dataSource.jndi.slave_1=java:comp/env/jdbc/slave_1
# dubbo 配置
dubbo.cache.file=/data/application/logs/shoufubang/dubbo/provider.cache
dubbo.registry.address=zk1-1.shoufubang:2181,zk1-2.shoufubang:2181,zk1-3.shoufubang:2181
dubbo.owner=

databaseType=mysql